{
  "coc.preferences.rootPatterns": ["tsconfig.json", ".vim",".git",".hg",".projections.json"],
  "codeLens.enable": true,
  "solargraph.symbols": true,
  "coc.preferences.formatOnSaveFiletypes": ["go", "css", "javascript", "typescript", "vue", "html", "scss", "ts" ],
  "prettier.disableLanguages": [],
  "list.maxPreviewHeight": 30,
  "prettier.semi": true,
  "prettier.trailingComma": "es5",
  "prettier.htmlWhitespaceSensitivity": "strict",
  "suggest.autoTrigger": "always",
  "vetur.useWorkspaceDependencies": true,
  "vetur.experimental.templateInterpolationService": false,
  "snippets.extends": {
    "typescript": ["javascript"]
  },

  "tsserver.trace.server":"verbose",
  "vetur.trace.server":"verbose",
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"],
      "initializationOptions": {
        "usePlaceholders": true
      }
    }
  },
	"list.insertMappings": {
		"<C-n>": "normal:j",
		"<C-p>": "normal:k",
		"<C-t>": "action:tabe"
	}
}

